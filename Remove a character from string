int j, count = 0, n = input.length(); 
    char []t = input.toCharArray(); 
    for (int i = j = 0; i < n; i++) 
    { 
        if (t[i] != c) 
        t[j++] = t[i]; 
        else
            count++; 
    } 
      
    while(count > 0) 
    { 
        t[j++] = '\0'; 
        count--; 
    } 
      
    return String.valueOf(t);
        
